generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Accounts {
  id      Int     @id @default(autoincrement())
  plaidId String?
  name    String
  userId  String
}

model Categories {
  id      Int     @id @default(autoincrement())
  plaidId String?
  name    String
  userId  String
}
